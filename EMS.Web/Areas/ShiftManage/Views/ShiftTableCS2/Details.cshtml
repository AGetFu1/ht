@{
    ViewBag.Title = "Form";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    var keyValue = $.request("keyValue");
    $(function () {
        //initControl();
        $.ajax({
            url: "/ShiftManage/ShiftTableCS2/GetFormJson",
            data: { keyValue: keyValue },
            dataType: "json",
            async: false,
            success: function (data) {
                $("#form").formSerialize(data);
                $("#form").find('.form-control,select,input').attr('readonly', 'readonly');
                $("#form").find('div.ckbox label').attr('for', '');
            }
        });
    });
   
</script>

<form id="form">
    <div style="padding-top: 20px; margin-right: 20px;">
        <table class="form">
            <tr>
                <th class="formTitle">姓名</th>
                <td class="formValue">
                    <input id="F_Name" name="F_Name" type="text" class="form-control required" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">组别</th>
                <td class="formValue">
                    <input id="F_Team" name="F_Team" type="text" class="form-control required" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">联系方式</th>
                <td class="formValue">
                    <input id="F_Phone" name="F_Phone" type="text" class="form-control required" />
                </td>
            </tr>
            <tr>
                <th class="formTitle">邮箱</th>
                <td class="formValue">
                    <input id="F_Email" name="F_Email" type="text" class="form-control required" />
                </td>
            </tr>
        </table>
    </div>
</form>
